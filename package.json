{
  "name": "@aglet/cartographer",
  "version": "v1.3.2",
  "description": "Map making lib for Aglet.io",
  "main": "dist/index.js",
  "author": "luetkemj",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luetkemj/aglet-cartographer"
  },
  "scripts": {
    "build": "yarn clean:dist && yarn test && yarn webpack",
    "build:storybook": "build-storybook",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "clean:storybook": "rimraf storybook-static",
    "clean": "yarn clean:coverage && yarn clean:dist && yarn clean:storybook && yarn clean:node_modules",
    "dev": "yarn storybook",
    "flow": "./node_modules/.bin/flow",
    "lint:eslint": "eslint .",
    "lint:sass-lint": "sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q",
    "lint": "yarn lint:eslint && yarn lint:sass-lint",
    "prepublish": "yarn build",
    "reinstall": "yarn clean && yarn",
    "storybook": "start-storybook -p 6006",
    "test:watch": "jest --watch --coverage",
    "test": "yarn lint && jest --coverage && yarn flow",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.prod.js"
  },
  "dependencies": {
    "canvas": "1.6.7",
    "pixi.js": "4.5.3",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "3.2.17",
    "@storybook/addon-actions": "3.2.17",
    "@storybook/addon-info": "3.2.17",
    "@storybook/addon-links": "3.2.17",
    "@storybook/addon-options": "3.2.17",
    "@storybook/react": "3.2.17",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "4.2.3",
    "css-loader": "0.28.7",
    "eslint": "4.12.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "0.60.1",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "node-sass": "4.7.2",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "svg-url-loader": "2.3.0",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.6"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
